<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Main" Color="#FF9700"></SolidColorBrush>
    <SolidColorBrush x:Key="Secondary" Color="#FFC673"></SolidColorBrush>
    <SolidColorBrush x:Key="Back" Color="#FFE573" Opacity=".2"></SolidColorBrush>
    <SolidColorBrush x:Key="BackAsset" Color="#FFC255" Opacity=".2"></SolidColorBrush>
    <Style TargetType="TextBlock" x:Key="Text">
        <Setter Property="Foreground" Value="#FF9700"></Setter>
    </Style>
    <Style TargetType="ListView">
        <Setter Property="Foreground" Value="#FF9700"></Setter>
    </Style>
    <SolidColorBrush x:Key="TextColor" Color="#FF9700"></SolidColorBrush>
    <Image Source="icons8-sun-40.png" x:Key="Theme"></Image>
</ResourceDictionary>