<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net6.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <Resource Include="Assets\Fonts\Poppins-ExtraLight.ttf" />
      <None Remove="Assets\icons8-refresh-50.png" />
      <Resource Include="Assets\icons8-refresh-50.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="Assets\icons8-search-40.png" />
      <Resource Include="Assets\icons8-search-40.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      <None Remove="Assets\icons8-back-50.png" />
      <Resource Include="Assets\icons8-back-50.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      
      <Resource Include="Assets\Fonts\AnekLatin-Regular.ttf" />
      
      <None Remove="Assets\icons8-sun-50.png" />
      
      <None Remove="Assets\icons8-sun-40.png" />
      
      <Resource Include="Assets\icons8-sun-40.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
      
      <None Remove="Assets\icons8-moon-symbol-40.png" />
      
      <Resource Include="Assets\icons8-moon-symbol-40.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
    </ItemGroup>

    <ItemGroup>
      
      <Resource Include="Assets\icons8-home-40.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Resource>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Assets\icons8-moon-symbol-50.png" />
    </ItemGroup>

</Project>
